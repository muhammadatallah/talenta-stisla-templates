{
    "name": "infyomlabs/stisla-templates",
    "type": "library",
    "description": "Stisla templates for InfyOm Laravel Generator",
    "keywords": [
        "infyom",
        "laravel",
        "generator",
        "stisla",
        "stisla-ui",
        "templates",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Atallah",
            "email": "m.atallah@talenta.com.co"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0",
        "infyomlabs/laravel-ui-stisla": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "InfyOm\\StislaTemplates\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\InfyOm\\StislaTemplates\\StislaTemplatesServiceProvider"
            ]
        }
    },
    "funding": [
        {
            "type": "opencollective",
            "url": "https://opencollective.com/infyomlabs"
        }
    ]
}